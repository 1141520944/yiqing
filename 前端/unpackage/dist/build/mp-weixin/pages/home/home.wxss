.qr-box{width:400rpx;height:460rpx;margin:0 auto;margin-top:20rpx}.flex-center{display:flex;flex-direction:row;justify-content:center}.UCenter-bg{background-color:#6cf;height:400rpx;display:flex;justify-content:center;padding-top:40rpx;overflow:hidden;position:relative;flex-direction:column;align-items:center;color:#fff;font-weight:300;text-shadow:0 0 3px rgba(0,0,0,.3)}.UCenter-bg text{opacity:.8}.UCenter-bg image{width:200rpx;height:200rpx}.UCenter-bg .gif-wave{position:absolute;width:100%;bottom:0;left:0;z-index:99;mix-blend-mode:screen;height:100rpx}