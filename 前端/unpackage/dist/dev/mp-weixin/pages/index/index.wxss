/* 标题栏 */
.title {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: auto;
  padding-top: 25px;
  z-index: 10;
  background-color: rgba(66, 185, 131, 0);
  color: rgba(255, 255, 255, 0.8);
}
.title > view {
  height: 44px;
}
.title .box1 {
  width: 60rpx;
  margin: 0 40rpx;
  font-size: 36rpx;
}
.title .tab > view {
  margin: 0 30rpx;
  line-height: 64rpx;
  font-size: 36rpx;
  position: relative;
  letter-spacing: 0;
  transition: -webkit-transform 0.3s ease-in-out 0s;
  transition: transform 0.3s ease-in-out 0s;
  transition: transform 0.3s ease-in-out 0s, -webkit-transform 0.3s ease-in-out 0s;
  -webkit-transform: scale(1, 1);
          transform: scale(1, 1);
}
.title .tab > view.active {
  -webkit-transform: scale(1.15, 1.15);
          transform: scale(1.15, 1.15);
}
.title.style2 {
  color: #FFF;
  background-color: #5064eb;
}
.title.style2 .tab > view.active {
  color: #FFF;
}

